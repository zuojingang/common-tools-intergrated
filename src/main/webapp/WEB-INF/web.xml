<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	version="3.0">
	<display-name>java-utils-intergrated</display-name>
	<servlet>
		<servlet-name>GraphVerifyCodeShow</servlet-name>
		<servlet-class>pers.zuo.controller.GraphVerifyCodeShow</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GraphVerifyCodeShow</servlet-name>
		<url-pattern>/verify-code/graph/show</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>